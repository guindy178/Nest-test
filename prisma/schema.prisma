// schema.prisma

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}
model Product {
  id Int      @id @default(autoincrement())
  name String
  price String
  description String
  createdAt DateTime @default(now())
  createdBy String
  active Boolean
}
